#include<stdio.h>
#include<math.h>

int main ()
{
    //here I am initializing type double variables
    double a, b, c, d, e, f, g, h;
    //here I politely ask user to input a and b
    printf("input a");
    scanf("%lf", &a);
    printf("input b");
    scanf("%lf", &b);
    //I've divided one big formula into few ones that are smaller
    c=a - b;
    d=pow(c, 3);
    e=pow(a, 3) - 3*a*a*b;
    f=d - e;
    g=pow(b, 3) - 3*a*b*b;
    h=f/g;
    printf("%lf", h);
    //here the result is being printed
    return 0;
}
